> ## random atomic sequences
> set.seed(20150921)
> 
> f <- tempfile()
> local({
+ n <- 1000
+ l <- 30
+ k <- 5
+ 
+ s <- lapply(sample(1:l, n, TRUE), function(x)
+        sample(1:k, x, TRUE)
+ )
+ 
+ m <- sapply(s, length)
+ s <- cbind(
+     rep(1:n, m),
+     unlist(lapply(m, seq)),
+     unlist(s)
+ )
+ 
+ write.table(s, file = f, row.names = FALSE, col.names = FALSE)
+ })
> 
> ## use generated data
> t <- read_baskets(con  = f,
+                   info = c("sequenceID", "eventID"))
> unlink(f)
> 
